<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="triangle" class="com.topyfi.javabrains.Triangle" scope="prototype"> 
		<property name="points">
		<list>
			<ref bean="zeroPoint" />
			<ref bean="point2" />
			<ref bean="point3" />
		</list>
		</property> 
	</bean>
	
	<bean id="zeroPoint" class="com.topyfi.javabrains.Point">
		<property name="x" value="0" />
		<property name="y" value="0" />
	</bean>
	<bean id="point2" class="com.topyfi.javabrains.Point">
		<property name="x" value="-20" />
		<property name="y" value="0" />
	</bean>
	<bean id="point3" class="com.topyfi.javabrains.Point">
		<property name="x" value="23 " />
		<property name="y" value="0" />
	</bean>


</beans>





